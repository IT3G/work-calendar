{
  "name": "work-calendar",
  "version": "0.0.0",
  "scripts": {
    "preBuild": "node scripts/build-pre.js",
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "startMobileDebug": "ng serve --host 0.0.0.0 --proxy-config proxy.conf.json",
    "build": "npm run preBuild && ng build",
    "build:prod": "npm run preBuild && ng build --configuration=production",
    "build:preprod": "npm run preBuild && ng build --configuration=preprod",
    "build:test": "npm run preBuild && ng build --configuration=test --base-href /work-calendar/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.14",
    "@angular/cdk": "^10.1.3",
    "@angular/common": "~10.0.14",
    "@angular/compiler": "~10.0.14",
    "@angular/core": "~10.0.14",
    "@angular/forms": "~10.0.14",
    "@angular/localize": "^10.0.14",
    "@angular/material": "^10.1.3",
    "@angular/platform-browser": "^10.0.14",
    "@angular/platform-browser-dynamic": "~10.0.14",
    "@angular/pwa": "^0.1000.8",
    "@angular/router": "~10.0.14",
    "@angular/service-worker": "~10.0.14",
    "@ng-bootstrap/ng-bootstrap": "^7.0.0",
    "angular-epic-spinners": "^2.0.0",
    "bootstrap": "^4.5.2",
    "chart.js": "^2.9.3",
    "core-js": "^3.6.5",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
