{
  "name": "work-calendar-backend",
  "version": "0.0.1",
  "description": "",
  "engines": {
    "node": "12.x",
    "yarn": "1.x"
  },
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:prod": "yarn prebuild && cross-env NODE_ENV=prod webpack",
    "build:preprod": "yarn prebuild && cross-env NODE_ENV=preprod webpack",
    "heroku-postbuild": "yarn prebuild && cross-env NODE_ENV=test webpack",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=dev nest start --watch",
    "start:test": "cross-env NODE_ENV=test nest start --watch",
    "start:debug": "cross-env NODE_ENV=dev nest start --debug --watch",
    "start:nodeserver": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.7.2",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/mongoose": "^6.1.2",
    "@nestjs/platform-express": "^6.7.2",
    "@nestjs/swagger": "^3.1.0",
    "body-parser": "^1.19.0",
    "cfenv": "^1.2.2",
    "commander": "^3.0.2",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.2.0",
    "i": "^0.3.6",
    "ldapjs": "^1.0.2",
    "lodash-es": "^4.17.15",
    "mongoose": "^5.7.7",
    "nest": "^0.1.6",
    "nodemailer": "^6.3.1",
    "npm": "^6.13.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "swagger-ui-express": "^4.1.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.14.0",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.7.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/mongoose": "^5.5.32",
    "@types/node": "^12.7.5",
    "@types/supertest": "^2.0.8",
    "copy-webpack-plugin": "^5.0.5",
    "cross-env": "^6.0.3",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
